{
  // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
  // 기존 특성에 대한 설명을 보려면 가리킵니다.
  // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
  "version": "0.2.0",
  "configurations": [
    // Node.js를 사용하여 실행 및 디버그
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Backend",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\back-end\\app.js"
    },
    // Chrome을 사용하여 실행 및 디버그
    // 수동 실행을 위해 Chrome을 먼저 시작하고 디버거를 연결하려면
    // "url"을 사용합니다.
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Frontend",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}\\front-end",
    }
    // {
    //   "name": "Attach to Process",
    //   "type": "node",
    //   "request": "attach",
    //   "processId": "${command:PickProcess}"
    // },    

  ]
}